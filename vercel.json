{
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/index.js" },
    { "source": "/health", "destination": "/api/index.js" }
  ],
  "redirects": [
    { "source": "/", "destination": "/health", "permanent": false }
  ]
}


